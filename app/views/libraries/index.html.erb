<h1>My Library</h1>
<%= will_paginate @books %>
<table class="table table-hover">
  <thead>
    <tr>
      <th>No.</th>
      <th>Image</th>
      <th>Name</th>
      <th>Title</th>
      <th>Author</th>
      <th>Description</th>
      <th>Content</th>
    </tr>
  </thead>
  <tbody>
    <%= render @books %>
  </tbody>
</table>